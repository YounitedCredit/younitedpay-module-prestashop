{
    "name": "module-partners/younitedpay",
    "description": "PrestaShop module Younited Pay",
    "homepage": "https://www.younited-pay.com/",
    "license": "OSL-3.0",
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "prepend-autoloader": false
    },
    "require": {
        "php": ">=5.6.0",
        "prestashop/module-lib-service-container": "^1.3"
    },
    "require-dev": {
        "prestashop/php-dev-tools": "^3.4",
        "phpunit/phpunit": "^5",
        "mockery/mockery": "^1.2",
        "prestashop/header-stamp": "^1.7"
    },
    "autoload-dev": {
        "psr-4": {
            "YounitedpayClasslib\\": "vendor/totpsclasslib/src/",
            "YounitedpayAddon\\": "src/",
            "YounitedPaySDK\\": "vendor/younitedpaysdk/src/",
            "Tests\\": "202/tests/"
        },
        "classmap": [
            "controllers/",
            "younitedpay.php"
        ],
        "files": [],
        "exclude-from-classmap": [
            "vendor/symfony/polyfill-php80/Resources/stubs/Attribute.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "YounitedpayClasslib\\": "vendor/totpsclasslib/src/",
            "YounitedPaySDK\\": "vendor/younitedpaysdk/src/",
            "YounitedpayAddon\\": "src/"
        },
        "classmap": [
            "controllers/",
            "younitedpay.php"
        ],
        "files": [],
        "exclude-from-classmap": [
            "vendor/symfony/polyfill-php80/Resources/stubs/Attribute.php"
        ]
    },
    "scripts": {
        "pre-autoload-dump": "if [ ${TOTPSCLASSLIB_DEV_PATH} ]; then php ${TOTPSCLASSLIB_DEV_PATH}/classlib/refresh.php .; find  ./vendor/totpsclasslib -name '*.php' -print0 | xargs -0  sed -ri -e 's!ounitedpay-prestashop!ounitedpay!g'; find  ./vendor/composer -name '*.php' -print0 | xargs -0  sed -ri -e 's!ounitedpay-prestashop!ounitedpay!g' ; rm ./controllers/admin/AdminYounitedpay-prestashopProcessLogger.php ; fi"
    },
    "type": "prestashop-module",
    "author": "PrestaShop"
}